background-color:whit;
}
/* Style for the QTabBar (the row of tabs) */
QTabBar::tab {
    background-color: #737373;  
    color: white;             
    border: 2px solid black;  
    border-radius: 10px;      
     margin-right: 10px; 
    padding: 10px 20px;         
    min-width: 100px;         
    font-size: 16px;            
}

/* Hover effect for tabs */
QTabBar::tab:hover {
    background-color: grey;  /* Change background on hover */
    color: white;               /* Text color on hover */
}

/* Active (selected) tab styling */
QTabBar::tab:selected {
    background-color: grey;  /* Background for selected tab */
    color: white;               /* Text color for selected tab */
   /* border: 2px solid black; /* Highlight the bottom border */
}

/* Disabled tab styling */
QTabBar::tab:disabled {
    background-color:#737373;  /* Gray out disabled tabs */
    color: #888888;             /* Text color for disabled tabs */
}

/* Style for the widget area (content inside the tabs) */
QWidget {
    background-color: rgb(0, 85, 0);;  /* Background for the tab content */
    border: none;
    border-radius: 10px;        /* Rounded corners for the content */
   /* padding: 10px;*/
}
QTabWidget::pane {
    padding: 10px;  /* Apply padding only to the QTabWidget pane */
}

QTableWidget {
    margin: 0px;  /* Ensure no margin/padding inside the QTableWidget */
}
QTableWidget {
    background-color: #a6c868;   /* Set a distinct background for the table */
    border: 1px solid black;   /* Add a border for better visibility */
}

QTableWidget::item {
    background-color: #a6c868;   /* Set item background */
